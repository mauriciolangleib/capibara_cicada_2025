(-2, 2, 200),\n","                                   np.linspace(-2, 2, 200))\n","all_losses = np.zeros_like(phi0_mesh)\n","\n","# Función de pérdida en el subespacio\n","def compute_loss_phi(phi0, phi1, X, y, phi, theta_0, theta_W):\n","    phi_temp = phi.copy()\n","    phi_temp[0] = phi1\n","    y_prob, _, _ = shallow_30_3_1(X, phi0, phi_temp, theta_0, theta_W)\n","    return binary_cross_entropy(y, y_prob)\n","\n","# Calcular heatmap de pérdidas (train completo)\n","for idx, _ in np.ndenumerate(phi0_mesh):\n","    all_losses[idx] = compute_loss_phi(phi0_mesh[idx],\n","                                       phi1_mesh[idx],\n","                                       X_train.values,\n","                                       y_train.values,\n","                                       phi, theta_0, theta_W)\n","\n","# Trayectoria SGD proyectada\n","phi0_traj = [p[0] for p in trajectory_sgd]\n","phi1_traj = [p[1][0] for p in trajectory_sgd]\n","\n","# Plot\n","plt.figure(figsize=(7,5))\n","plt.contourf(phi0_mesh, phi1_mesh, all_losses, levels=50, cmap=\"viridis\")\n","plt.plot(phi0_traj, phi1_traj, \"r.-\", markersize=3)\n","plt.xlabel(\"phi_0\")\n","plt.ylabel(\"phi[0]\")\n","plt.title(\"Trayectoria SGD en el paisaje de pérdida\")\n","plt.colorbar(label=\"Binary cross-entropy\")\n","plt.show()\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":488},"id":"GhuG6D03idCh","executionInfo":{"status":"ok","timestamp":1756820017375,"user_tz":180,"elapsed":4188,"user":{"displayName":"Felipe Maresca","userId":"00134575873646022799"}},"outputId":"6c38ef89-8aee-40c1-c979-619cebc7bc13"},"execution_count":null,"outputs":[{"output_type":"display_data","data":{"text/plain":["<Figure size 700x500 with 2 Axes>"],"image/png":"\n"},"metadata":{}}]},{"cell_type":"code","source":["# Extraer últimos parámetros entrenados con SGD\n","phi_0_sgd, phi_sgd, theta_0_sgd, theta_W_sgd = trajectory_sgd[-1]\n","\n","# Forward pass en train y test\n","y_train_prob_sgd, _, _ = shallow_30_3_1(X_train.values, phi_0_sgd, phi_sgd, theta_0_sgd, theta_W_sgd)\n","y_test_prob_sgd, _, _  = shallow_30_3_1(X_test.values, phi_0_sgd, phi_sgd, theta_0_sgd, theta_W_sgd)\n","\n","# Predicciones binarias\n","y_train_pred_sgd = (y_train_prob_sgd >= 0.5).astype(int)\n","y_test_pred_sgd = (y_test_prob_sgd >= 0.5).astype(int)\n","\n","# Accuracy\n","acc_train_sgd = accuracy(y_train.values, y_train_pred_sgd)\n","acc_test_sgd = accuracy(y_test.values, y_test_pred_sgd)\n","\n","print(f\"Accuracy TRAIN (SGD): {acc_train_sgd:.3f}\")\n","print(f\"Accuracy TEST (SGD): {acc_test_sgd:.3f}\")\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"y9D3gD7Li7mQ","executionInfo":{"status":"ok","timestamp":1756820017376,"user_tz":180,"elapsed":8,"user":{"displayName":"Felipe Maresca","userId":"00134575873646022799"}},"outputId":"f4f0bd59-3778-45cd-896d-a5dcbf5d42d3"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["Accuracy TRAIN (SGD): 0.987\n","Accuracy TEST (SGD): 0.974\n"]}]},{"cell_type":"markdown","source":["##Redes Neuronales Profundas\n","\n"],"metadata":{"id":"vcDZHH1qNnLN"}},{"cell_type":"code","source":["np.random.seed(0)\n","\n","# Capa 1: 30 -> 2\n","theta1_W = np.random.randn(30,2) * 0.1\n","theta1_b = np.random.randn(2) * 0.1\n","\n","# Capa 2: 2 -> 2\n","theta2_W = np.random.randn(2,2) * 0.1\n","theta2_b = np.random.randn(2) * 0.1\n","\n","# Capa salida: 2 -> 1\n","phi_W = np.random.randn(2) * 0.1\n","phi_0 = 0.0\n"],"metadata":{"id":"0oMdgDliNl2Z"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["def deep_2x2_1_forward(X, theta1_W, theta1_b, theta2_W, theta2_b, phi_W, phi_0):\n","    \"\"\"\n","    Deep NN: 30 -> 2 -> 2 -> 1\n","    X: (n_samples,30)\n","    theta1_W, theta2_W: pesos\n","    theta1_b, theta2_b: bias\n","    phi_W: pesos salida\n","    phi_0: bias salida\n","    \"\"\"\n","    # Capa oculta 1\n","    pre1 = X @ theta1_W + theta1_b\n","    act1 = relu(pre1)\n","\n","    # Capa oculta 2\n","    pre2 = act1 @ theta2_W + theta2_b\n","    act2 = relu(pre2)\n","\n","    # Salida\n","    z = phi_0 + act2 @ phi_W\n","    y_prob = 1 / (1 + np.exp(-z))\n","\n","    return y_prob, (pre1, act1, pre2, act2)\n"],"metadata":{"id":"bXvqNcfbNpEY"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["y_train_prob, _ = deep_2x2_1_forward(X_train.values, theta1_W, theta1_b,\n","                                      theta2_W, theta2_b, phi_W, phi_0)\n","y_test_prob, _  = deep_2x2_1_forward(X_test.values, theta1_W, theta1_b,\n","                                     theta2_W, theta2_b, phi_W, phi_0)\n","\n","y_train_pred = (y_train_prob >= 0.5).astype(int)\n","y_test_pred  = (y_test_prob >= 0.5).astype(int)\n","\n","print(\"Accuracy TRAIN:\", accuracy(y_train.values, y_train_pred))\n","print(\"Accuracy TEST: \", accuracy(y_test.values, y_test_pred))\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"fA57z8hANtUR","executionInfo":{"status":"ok","timestamp":1756820017389,"user_tz":180,"elapsed":11,"user":{"displayName":"Felipe Maresca","userId":"00134575873646022799"}},"outputId":"8a9eab9a-4e93-4b2c-c119-b998624752e5"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["Accuracy TRAIN: 0.6285714285714286\n","Accuracy TEST:  0.6228070175438597\n"]}]},{"cell_type":"code","source":["def binary_cross_entropy(y_true, y_prob):\n","    eps = 1e-8\n","    return -np.mean(y_true * np.log(y_prob+eps) + (1-y_true)*np.log(1-y_prob+eps))\n"],"metadata":{"id":"9j1iu6gPRzTc"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["# Hiperparámetros\n","lr = 0.1\n","n_steps = 600\n","X_train_vals = X_train.values\n","y_train_vals = y_train.values\n","\n","# Inicialización\n","np.random.seed(0)\n","theta1_W = np.random.randn(30,2) * 0.5\n","theta1_b = np.random.randn(2) * 0.5\n","theta2_W = np.random.randn(2,2) * 0.5\n","theta2_b = np.random.randn(2) * 0.5\n","phi_W = np.random.randn(2) * 0.5\n","phi_0 = 0.0\n","\n","losses = []\n","\n","for step in range(n_steps):\n","  # -------------------------------\n","  # Forward pass\n","  # -------------------------------\n","  y_prob, (pre1, act1, pre2, act2) = deep_2x2_1_forward(\n","      X_train_vals, theta1_W, theta1_b, theta2_W, theta2_b, phi_W, phi_0\n","  )\n","\n","  # -------------------------------\n","  # Compute loss\n","  # -------------------------------\n","  loss = binary_cross_entropy(y_train_vals, y_prob)\n","  losses.append(loss)\n","\n","  # -------------------------------\n","  # Gradientes capa de salida (Output Layer)\n","  # -------------------------------\n","  # Loss → z (pre-activación de salida)\n","  dz = y_prob - y_train_vals                  # dz = dL/dz\n","\n","  # Gradiente bias de salida\n","  dphi_0 = np.mean(dz)                        # dL/d(phi_0)\n","\n","  # Gradiente pesos de salida\n","  dphi_W = np.mean(dz[:,None] * act2, axis=0) # dL/d(phi_W)\n","  #                       ↑ multiplicación elemento a elemento con activaciones de capa 2\n","\n","  # -------------------------------\n","  # Gradientes capa oculta 2 (Hidden Layer 2)\n","  # -------------------------------\n","  # Gradiente de activaciones de capa 2 (propagando desde salida)\n","  dact2 = dz[:,None] * phi_W                  # dL/d(act2)\n","  #                   ↑ multiplicación con pesos de salida (cadena)\n","\n","  # Gradiente pre-activaciones capa 2 (ReLU)\n","  dpre2 = dact2 * (pre2 > 0)                 # dL/d(pre2)\n","  #                       ↑ derivada de ReLU (0 si pre2 ≤ 0, 1 si >0)\n","\n","  # Gradiente bias capa 2\n","  dtheta2_b = np.mean(dpre2, axis=0)         # dL/d(theta2_b)\n","\n","  # Gradiente pesos capa 2\n","  dtheta2_W = act1.T @ dpre2 / X_train_vals.shape[0]  # dL/d(theta2_W)\n","  #                   ↑ multiplicación con activaciones de capa 1\n","\n","  # -------------------------------\n","  # Gradientes capa oculta 1 (Hidden Layer 1)\n","  # -------------------------------\n","  # Gradiente de activaciones capa 1\n","  dact1 = dpre2 @ theta2_W.T                  # dL/d(act1)\n","  #                       ↑ cadena hacia capa 1 mediante pesos de capa 2\n","\n","  # Gradiente pre-activaciones capa 1 (ReLU)\n","  dpre1 = dact1 * (pre1 > 0)                 # dL/d(pre1)\n","\n","  # Gradiente bias capa 1\n","  dtheta1_b = np.mean(dpre1, axis=0)         # dL/d(theta1_b)\n","\n","  # Gradiente pesos capa 1\n","  dtheta1_W = X_train_vals.T @ dpre1 / X_train_vals.shape[0]  # dL/d(theta1_W)\n","\n","  # -------------------------------\n","  # Update de parámetros (Gradient Descent)\n","  # -------------------------------\n","  phi_0 -= lr * dphi_0\n","  phi_W -= lr * dphi_W\n","  theta2_b -= lr * dtheta2_b\n","  theta2_W -= lr * dtheta2_W\n","  theta1_b -= lr * dtheta1_b\n","  theta1_W -= lr * dtheta1_W"],"metadata":{"id":"b69noHHZR0wo"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["y_train_prob, _ = deep_2x2_1_forward(X_train_vals, theta1_W, theta1_b,\n","                                     theta2_W, theta2_b, phi_W, phi_0)\n","y_test_prob, _  = deep_2x2_1_forward(X_test.values, theta1_W, theta1_b,\n","                                     theta2_W, theta2_b, phi_W, phi_0)\n","\n","y_train_pred = (y_train_prob >= 0.5).astype(int)\n","y_test_pred  = (y_test_prob >= 0.5).astype(int)\n","\n","print(\"Accuracy TRAIN:\", accuracy(y_train.values, y_train_pred))\n","print(\"Accuracy TEST: \", accuracy(y_test.values, y_test_pred))\n","\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"V6IbxeaNR46n","executionInfo":{"status":"ok","timestamp":1756820159680,"user_tz":180,"elapsed":13,"user":{"displayName":"Felipe Maresca","userId":"00134575873646022799"}},"outputId":"2fb512de-b271-46d2-b055-0b9718f5edc0"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["Accuracy TRAIN: 0.9714285714285714\n","Accuracy TEST:  0.9473684210526315\n"]}]},{"cell_type":"code","source":["plt.plot(losses)\n","plt.xlabel(\"Step\")\n","plt.ylabel(\"Binary Cross-Entropy Loss\")\n","plt.title(\"Evolución de la loss durante Gradient Descent\")\n","plt.show()\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":472},"id":"8QYorbbsR7Ai","executionInfo":{"status":"ok","timestamp":1756820161706,"user_tz":180,"elapsed":141,"user":{"displayName":"Felipe Maresca","userId":"00134575873646022799"}},"outputId":"80867a05-6171-42f1-86be-d9a834915027"},"execution_count":null,"outputs":[{"output_type":"display_data","data":{"text/plain":["<Figure size 640x480 with 1 Axes>"],"image/png":"\n"},"metadata":{}}]},{"cell_type":"code","source":["# --- SGD para deep 30 -> 2 -> 2 -> 1 ---\n","import numpy as np\n","\n","# Hiperparámetros\n","# Modifiquen los valores y observen que sucede\n","lr = 0.1\n","n_epochs = 20\n","batch_size = 16\n","\n","X_train_vals = X_train.values\n","y_train_vals = y_train.values\n","n_samples = X_train_vals.shape[0]\n","\n","# Inicialización\n","np.random.seed(0)\n","theta1_W = np.random.randn(30,2) * 0.1\n","theta1_b = np.random.randn(2) * 0.1\n","theta2_W = np.random.randn(2,2) * 0.1\n","theta2_b = np.random.randn(2) * 0.1\n","phi_W = np.random.randn(2) * 0.1\n","phi_0 = 0.0\n","\n","losses_sgd = []\n","\n","for epoch in range(n_epochs):\n","    indices = np.arange(n_samples)\n","    np.random.shuffle(indices)  # barajar los datos cada epoch\n","\n","    for start in range(0, n_samples, batch_size):\n","        batch_idx = indices[start:start+batch_size]\n","        X_batch = X_train_vals[batch_idx]\n","        y_batch = y_train_vals[batch_idx]\n","\n","        # Forward\n","        y_prob, (pre1, act1, pre2, act2) = deep_2x2_1_forward(X_batch,\n","                                                              theta1_W, theta1_b,\n","                                                              theta2_W, theta2_b,\n","                                                              phi_W, phi_0)\n","        # Loss\n","        loss = binary_cross_entropy(y_batch, y_prob)\n","        losses_sgd.append(loss)\n","\n","        # Gradientes salida\n","        dz = y_prob - y_batch\n","        dphi_0 = np.mean(dz)\n","        dphi_W = np.mean(dz[:,None] * act2, axis=0)\n","\n","        # Gradientes capa oculta 2\n","        dact2 = dz[:,None] * phi_W\n","        dpre2 = dact2 * (pre2 > 0)\n","        dtheta2_b = np.mean(dpre2, axis=0)\n","        dtheta2_W = act1.T @ dpre2 / X_batch.shape[0]\n","\n","        # Gradientes capa oculta 1\n","        dact1 = dpre2 @ theta2_W.T\n","        dpre1 = dact1 * (pre1 > 0)\n","        dtheta1_b = np.mean(dpre1, axis=0)\n","        dtheta1_W = X_batch.T @ dpre1 / X_batch.shape[0]\n","\n","        # Update SGD\n","        phi_0 -= lr * dphi_0\n","        phi_W -= lr * dphi_W\n","        theta2_b -= lr * dtheta2_b\n","        theta2_W -= lr * dtheta2_W\n","        theta1_b -= lr * dtheta1_b\n","        theta1_W -= lr * dtheta1_W\n","\n","# Evaluación final\n","y_train_prob_sgd, _ = deep_2x2_1_forward(X_train_vals, theta1_W, theta1_b,\n","                                         theta2_W, theta2_b, phi_W, phi_0)\n","y_test_prob_sgd, _  = deep_2x2_1_forward(X_test.values, theta1_W, theta1_b,\n","                                         theta2_W, theta2_b, phi_W, phi_0)\n","\n","y_train_pred_sgd = (y_train_prob_sgd >= 0.5).astype(int)\n","y_test_pred_sgd = (y_test_prob_sgd >= 0.5).astype(int)\n","\n","print(\"Accuracy TRAIN (SGD):\", accuracy(y_train.values, y_train_pred_sgd))\n","print(\"Accuracy TEST (SGD) :\", accuracy(y_test.values, y_test_pred_sgd))\n","\n","# Evolución de la loss\n","import matplotlib.pyplot as plt\n","plt.plot(losses_sgd)\n","plt.xlabel(\"Step\")\n","plt.ylabel(\"Binary Cross-Entropy Loss\")\n","plt.title(\"Evolución de la loss durante SGD\")\n","plt.show()\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":507},"id":"JR_k__pOkpgI","executionInfo":{"status":"ok","timestamp":1756820164299,"user_tz":180,"elapsed":294,"user":{"displayName":"Felipe Maresca","user