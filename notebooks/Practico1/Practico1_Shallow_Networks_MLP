Ow"}},{"cell_type":"code","source":["def binary_cross_entropy(y_true, y_pred):\n","    \"\"\"\n","    y_true: vector de valores reales (0/1)\n","    y_pred: vector de probabilidades (0-1)\n","    \"\"\"\n","    eps = 1e-9  # para evitar log(0)\n","    y_pred = np.clip(y_pred, eps, 1 - eps)\n","    return -np.mean(y_true * np.log(y_pred) + (1 - y_true) * np.log(1 - y_pred))\n"],"metadata":{"id":"RrTp1Z-k9AwN"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["# Inicialización\n","np.random.seed(0)\n","theta_W = np.random.randn(30,3) * 0.1\n","theta_0 = np.random.randn(3) * 0.1\n","phi = np.random.randn(3) * 0.1\n","phi_0 = 0.0\n","\n","lr = 0.1\n","n_steps = 150\n","trajectory = [(phi_0, phi.copy(), theta_0.copy(), theta_W.copy())]\n","\n","for step in range(n_steps):\n","    # Forward\n","    # Forward\n","    y_prob, pre, act = shallow_30_3_1(X_train.values, phi_0, phi, theta_0, theta_W)\n","\n","  # Gradientes\n","    dz = y_prob - y_train.values\n","\n","    dphi_0 = np.mean(dz)\n","    dphi = np.mean(dz[:, None] * act, axis=0)\n","\n","    # Gradiente capa oculta\n","    dpre = dz[:, None] * phi[None, :] * (pre > 0)  # ReLU derivada\n","    dtheta_0 = np.mean(dpre, axis=0)\n","    dtheta_W = X_train.values.T @ dpre / X_train.shape[0]\n","\n","    # Update\n","    phi_0 -= lr * dphi_0\n","    phi -= lr * dphi\n","    theta_0 -= lr * dtheta_0\n","    theta_W -= lr * dtheta_W\n","\n","    trajectory.append((phi_0, phi.copy(), theta_0.copy(), theta_W.copy()))\n"],"metadata":{"id":"xgmlHc4fE9x0"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["y_train_prob, _, _ = shallow_30_3_1(X_train.values, phi_0, phi, theta_0, theta_W)\n","y_test_prob, _, _  = shallow_30_3_1(X_test.values, phi_0, phi, theta_0, theta_W)\n","\n","y_train_pred = (y_train_prob >= 0.5).astype(int)\n","y_test_pred = (y_test_prob >= 0.5).astype(int)\n","\n","print(\"Accuracy TRAIN:\", accuracy(y_train.values, y_train_pred))\n","print(\"Accuracy TEST:\", accuracy(y_